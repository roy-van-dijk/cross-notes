{
  "manifest_version": 2,
  "name": "CrossNotes",
  "version": "0.3",
  "description": "Simple note taking extension",
  "permissions": [
      "storage"
   ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "96": "logo-96.png"
  }
}